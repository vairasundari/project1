import java.util.*;
public class Pattern1{
public static void main(String[] args){
int n,i,j,count;
Scanner s=new Scanner(System.in);
n=s.nextInt();
for(i=1;i<=n;i++){

if(i%2==0){
for(j=1;j<=n;j++)
System.out.print(j+" ");
System.out.println("\n");}

else {
for(j=n;j>=1;j--)
System.out.print(j+" ");
System.out.println("\n");}

}
}}
